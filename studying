public class project {
    public static void main(String[] args) {
        int a, b, c;
        {
            for (a = 1; b < 1000; a++) {
                for (b = a + 1; b < 1000; b++) {
                    c = 1000 - a - b;
                    if (a * a + b * b == c * c) {
                        System.out.println("");
                    }
                }

            }
            return 1;
        }
    }
}

