package com.example;

import java.util.Scanner;
import java.util.Vector;

public static void main(String[] args) {
        Vector<Integer> vec = new Vector<Integer>();// jest miejscem na liczby u≈ºytkownika
        Scanner scanner = new Scanner(System.in);//standardowy obiekt uzywany do wczytywania uzytkownika z klawiatury
        
        // wczytuje 10 liczb od uzytkownika  i zapisuje je w vec
        for (int i = 0; i < 10; i++) {
            System.out.print("podaj liczbe calkowita");
            int liczba = scanner.nextInt();
            vec.add(liczba);

        }


        for (int o : vec) {
            System.out.println(o);
        }

    }
}
